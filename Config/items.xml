<configs>
<append xpath="/items">

<item name="Enhanced Bow">
	<property name="Tags" value="weapon,ranged,bowDrawPenalty,archery,bow,perkArchery"/>
	<property name="DisplayType" value="rangedBow"/>
	<property name="HoldType" value="27"/> <!-- "Sway Off" is hardcoded for hold types 26, 27, 53 -->
	<property name="Meshfile" value="Items/Weapons/Ranged/Bows/WoodenBow/WoodenBowPrefab"/>
	<property name="Material" value="Mwood"/>
	<property name="RepairTools" value="resourceWood"/>
	<property name="FuelValue" value="300"/>
	<property name="EconomicValue" value="50"/>
	<property name="EconomicBundleSize" value="1"/>
	<property name="SellableToTrader" value="false"/>
	<property name="DegradationBreaksAfter" value="false"/>
	<property name="SoundJammed" value="ItemNeedsRepair"/>
	<property name="SoundDestroy" value="wooddestroy1"/>
	<property name="CrosshairOnAim" value="true"/> <!-- aimTest -->
	<property name="CrosshairUpAfterShot" value="false"/>
	<property name="ActionSkillGroup" value="Archery"/>
	<property name="CraftingSkillGroup" value="craftSkillWeapons"/>
	<property name="RepairExpMultiplier" value="10"/>
	<property name="PickupJournalEntry" value="alternateAmmoTip"/>
	<property class="Action0">
		<property name="Class" value="Catapult"/>
		<!-- Catapult might not be the best name for it but it can also be used for a slingshot -->
		<property name="Hitmask_override" value="Arrow"/>
		<property name="Delay" value="1"/> <!-- obsolete if rounds per minute exists -->
		<property name="Magazine_items" value="ammoArrowStone,ammoArrowIron,ammoArrowSteelAP,ammoArrowFlaming,Serration Arrow"/>
		<!-- <property name="Reload_time" value=".9"/> -->
		<property name="Sound_start" value="bow_fire"/>
		<property name="Sound_repeat" value=""/>
		<property name="Sound_end" value=""/>
		<property name="Sound_empty" value="weapon_empty"/>
		<property name="Sound_reload" value="bow_reload"/>
		<property name="Sound_draw" value="bow_draw"/>
		<property name="Sound_cancel" value="bow_fire_cancel"/>
		<property name="Particles_muzzle_fire" value="nozzleflashuzi"/>
		<property name="Particles_muzzle_smoke" value="nozzlesmokeuzi"/>
		<property name="Max_strain_time" value="0.5"/>
		<property name="ScopeOffset" value="-0.00062,0.088,0.065"/>
		<property name="SideOffset" value="0,0,0"/>
		<property name="BarrelOffset" value="0,0,0"/>
	</property>
	<property class="Action1"> <!-- UseAction -->
		<property name="Class" value="Zoom"/>
		<property name="Zoom_max_out" value="65"/>
		<property name="Zoom_max_in" value="65"/>
	</property>

	<property name="Group" value="Ammo/Weapons,Basics"/>
	<effect_group name="Base Effects"> <!-- gunWoodenBow -->
		<passive_effect name="DamageFalloffRange" operation="base_set" value="40"/>
		<passive_effect name="MaxRange" operation="base_set" value="150"/>
		<passive_effect name="MagazineSize" operation="base_set" value="1"/>

		<passive_effect name="EntityDamage" operation="perc_add" value="0"/>
		<passive_effect name="BlockDamage" operation="perc_add" value="0"/>

		<passive_effect name="SpreadDegreesVertical" operation="base_set" value="2.0"/>
		<passive_effect name="SpreadDegreesHorizontal" operation="base_set" value="2.0"/>
		<passive_effect name="SpreadMultiplierAiming" operation="base_set" value=".20"/>
		<passive_effect name="SpreadMultiplierCrouching" operation="base_set" value=".4"/>
		<passive_effect name="SpreadMultiplierWalking" operation="base_set" value="1.2"/>
		<passive_effect name="SpreadMultiplierRunning" operation="base_set" value="2.0"/>

		<passive_effect name="IncrementalSpreadMultiplier" operation="base_set" value="2"/>
		<passive_effect name="WeaponHandling" operation="base_set" value="0.5"/> <!-- crosshair reset speed -->

		<passive_effect name="RoundsPerMinute" operation="base_set" value="90"/>
		<passive_effect name="ReloadSpeedMultiplier" operation="base_set" value="1.2"/>

		<!--
		<passive_effect name="AttacksPerMinute" operation="perc_add" value="15"/>
		-->
		<!-- <passive_effect name="AttacksPerMinute" operation="base_set" value="166"/>

		AttacksPerMinute, RoundsPerMinute  do NOT affect bow rate of fire
		-->

		<passive_effect name="BurstRoundCount" operation="base_set" value="1"/>
		<passive_effect name="KickDegreesVertical" operation="base_set" value="2"/>
		<passive_effect name="KickDegreesHorizontal" operation="base_set" value="-1,1"/>
		<display_value name="dRecoil" value="15" />
		<passive_effect name="DegradationMax" operation="base_set" value="1000,4000" tier="1,6"/>
		<passive_effect name="ModSlots" operation="base_set" value="0,5" tier="1,6"/>
		<passive_effect name="ModPowerBonus" operation="perc_add" value="0.20" tags="EntityDamage"/>
		<passive_effect name="ModPowerBonus" operation="base_add" value="200" tags="EconomicValue"/>
	</effect_group>
</item>

<item name="Enhanced Crossbow">
	<property name="Tags" value="weapon,ranged,holdBreathAiming,reloadPenalty,sideAttachments,smallTopAttachments,mediumTopAttachments,perkArchery"/>
	<property name="DisplayType" value="rangedBow"/>
	<property name="HoldType" value="26"/>
	<property name="Meshfile" value="Items/Weapons/Ranged/Bows/Crossbow/crossbowPrefab"/>
	<property name="Material" value="Mmetal"/>
	<property name="RepairTools" value="resourceForgedIron"/>
	<property name="FuelValue" value="400"/>
	<property name="EconomicValue" value="1000"/>
	<property name="DegradationBreaksAfter" value="false"/>
	<property name="SoundJammed" value="ItemNeedsRepair"/>
	<property name="SoundDestroy" value="wooddestroy1"/>
	<property name="CrosshairOnAim" value="true"/> <!-- aimTest -->
	<property name="CrosshairUpAfterShot" value="true"/> <!-- aimTest -->
	<property name="Sound_Sight_In" value="crossbow_sight_in"/>
	<property name="Sound_Sight_Out" value="crossbow_sight_out"/>
	<property name="Group" value="Ammo/Weapons"/>
	<property name="ActionSkillGroup" value="Archery"/>
	<property name="CraftingSkillGroup" value="craftSkillWeapons"/>
	<property name="RepairExpMultiplier" value="5.5"/>
	<property name="PickupJournalEntry" value="alternateAmmoTip"/>
	<property class="Action0">
		<property name="Class" value="Launcher"/>
		<property name="Hitmask_override" value="Arrow"/>
		<property name="Delay" value="0.8"/> <!-- obsolete if rounds per minute exists -->
		<property name="Magazine_items" value="ammoCrossbowBoltStone,ammoCrossbowBoltIron, ammoCrossbowBoltSteelAP, ammoCrossbowBoltExploding, Serration Crossbolt"/>
		<!-- <property name="Reload_time" value="4"/> -->
		<property name="Sound_start" value="crossbow_fire"/>
		<property name="Sound_repeat" value=""/>
		<property name="Sound_end" value=""/>
		<property name="Sound_empty" value="weapon_empty"/>
		<property name="Sound_reload" value="crossbow_reload"/>
	</property>
	<property class="Action1"> <!-- UseAction -->
		<property name="Class" value="Zoom"/>
		<property name="Zoom_max_out" value="55"/>
		<property name="Zoom_max_in" value="55"/>
	</property>
	<effect_group name="Base Effects"> <!-- gunCrossbow -->
		<passive_effect name="DamageFalloffRange" operation="base_set" value="50"/>
		<passive_effect name="MaxRange" operation="base_set" value="200"/>
		<passive_effect name="MagazineSize" operation="base_set" value="1"/>
		<passive_effect name="ReloadSpeedMultiplier" operation="base_set" value=".5"/> <!-- 3.4s -->

		<!-- <passive_effect name="EntityDamage" operation="perc_add" value=".45"/>
		<passive_effect name="ProjectileVelocity" operation="perc_add" value="0.5"/>
		-->
		<passive_effect name="BlockDamage" operation="perc_add" value="0"/>

		<passive_effect name="SpreadDegreesVertical" operation="base_set" value="2.5"/>
		<passive_effect name="SpreadDegreesHorizontal" operation="base_set" value="2.5"/>
		<passive_effect name="SpreadMultiplierAiming" operation="base_set" value=".15"/>
		<passive_effect name="SpreadMultiplierCrouching" operation="base_set" value=".5"/>
		<passive_effect name="SpreadMultiplierWalking" operation="base_set" value="1.2"/>
		<passive_effect name="SpreadMultiplierRunning" operation="base_set" value="2.0"/>

		<passive_effect name="IncrementalSpreadMultiplier" operation="base_set" value="1.5"/>
		<passive_effect name="WeaponHandling" operation="base_set" value="0.3"/> <!-- crosshair reset speed -->

		<passive_effect name="RoundsPerMinute" operation="base_set" value="100"/>
		<passive_effect name="BurstRoundCount" operation="base_set" value="1"/>
		<passive_effect name="KickDegreesVertical" operation="base_set" value="2"/>
		<passive_effect name="KickDegreesHorizontal" operation="base_set" value="-1,1"/>
		<display_value name="dRecoil" value="15" />
		<passive_effect name="DegradationMax" operation="base_set" value="600,2350" tier="1,6"/>
		<passive_effect name="ModSlots" operation="base_set" value="0,5" tier="1,6"/>
		<passive_effect name="ModPowerBonus" operation="perc_add" value="0.25" tags="EntityDamage"/>
		<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
	</effect_group>
</item>

<!-- Serration Arrows -->
<item name="Serration Arrow">
	<property name="Extends" value="ammoArrowSteelAP"/>
	<property name="Meshfile" value="Items/Weapons/Ranged/Bows/WoodenBow/bodkinArrowPrefab"/>
	<property name="StickyMeshFile" value="Items/Weapons/Ranged/Bows/WoodenBow/bodkinArrowPrefab"/>
	<property name="EconomicValue" value="85"/>
	<property name="EconomicBundleSize" value="1"/>
	<property name="Damage_type" value="Piercing"/>
